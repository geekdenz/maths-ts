    "closure-compiler-wrapper": "^1.1.3",
    "expect": "^22.1.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.12",
    "ts-mocha": "^1.1.0",
    "typescript": "^2.7.1"
  },
  "dependencies": {}
